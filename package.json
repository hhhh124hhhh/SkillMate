{
  "name": "skill-mate",
  "private": true,
  "version": "2.0.0",
  "description": "SkillMate - Your AI Skill Partner. AI技能生态平台，支持技能构建、分享、售卖和学习",
  "author": {
    "name": "SkillMate Team",
    "email": "support@skillmate.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hhhh124hhhh/skill-mate.git"
  },
  "homepage": "https://github.com/hhhh124hhhh/skill-mate#readme",
  "bugs": {
    "url": "https://github.com/hhhh124hhhh/skill-mate/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "dev": "chcp 65001 && vite",
    "setup-python": "node scripts/setup-python.js",
    "generate-icons": "node scripts/generate-icons.js",
    "encrypt-skills": "cross-env NODE_ENV=production node scripts/encrypt-skills.js",
    "decrypt-skills": "cross-env NODE_ENV=development node scripts/encrypt-skills.js",
    "migrate-skills": "node scripts/migrate-skills.js migrate",
    "backup-skills": "node scripts/migrate-skills.js backup",
    "list-backups": "node scripts/migrate-skills.js list",
    "test:encryption": "node tests/run-encryption-tests.js",
    "prebuild": "npm run setup-python && npm run generate-icons && npm run encrypt-skills",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@modelcontextprotocol/server-filesystem": "^2026.1.14",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "electron-store": "^11.0.2",
    "fetcher-mcp": "^0.3.9",
    "file-saver": "^2.0.5",
    "immer": "^11.1.3",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "cross-env": "^7.0.3",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.1.2",
    "electron-updater": "^6.7.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "javascript-obfuscator": "^5.1.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.cjs",
  "build": {
    "appId": "ai.skillmate.desktop",
    "productName": "SkillMate",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "resources/skills/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "python-runtime",
        "to": "python-runtime",
        "filter": [
          "**/*",
          "!*.zip",
          "!get-pip.py",
          "!requirements.txt",
          "!**/__pycache__/**",
          "!**/*.pyc",
          "!**/*.dist-info/**",
          "!**/*.egg-info/**"
        ]
      },
      {
        "from": "resources/skills",
        "to": "resources/skills"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
